{
  "name": "spotify-wrapper-player",
  "version": "1.0.0",
  "description": "A Player using the vs-spotify-wrapper library",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*js",
    "prepush": "npm run lint && npm run test:coverage",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test",
    "start": "./node_modules/.bin/webpack-dev-server --open"
  },
  "nyc": {
    "check-coverage": true,
    "functions": 80,
    "lines": 80,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitorjsls30/spotify-wrapper-player.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Vitor Jos√© <vitorjsls30@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vitorjsls30/spotify-wrapper-player/issues"
  },
  "homepage": "https://github.com/vitorjsls30/spotify-wrapper-player#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "husky": "^0.14.3",
    "mocha": "^5.1.1",
    "nyc": "^12.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "vs-spotify-wrapper": "^2.0.1"
  }
}
